og('res: ', res);\r\n    // 请求成功 此处进行数据的批量处理\r\n    if (res.status === 200) {\r\n      // @ts-ignore\r\n      res.data.msg && message.info(res.data.msg)\r\n      return res.data as any;\r\n    }\r\n  },\r\n  (error: AxiosError) => {\r\n    localStorage.loadingStatus = false\r\n    store.dispatch(updateSettings(localStorage));\r\n    console.log('error: ', error.response)\r\n    // @ts-ignore\r\n    const { status } = error.response;\r\n    let msg = error.response?.data;\r\n    \r\n    console.log('status: ', status);\r\n    switch (status) {\r\n      case 400:\r\n        msg = '错误请求'\r\n        break\r\n      case 401:\r\n        msg = '登录失效，请重新登录'\r\n        store.dispatch(logout());\r\n        break\r\n      case 403:\r\n        msg = '无权访问'\r\n        break\r\n      case 404:\r\n        msg = '请求错误,未找到该资源'\r\n        break\r\n      case 405:\r\n        msg = '请求方法未允许'\r\n        break\r\n      case 504:\r\n        msg = '请求超时'\r\n        break\r\n      case 422:\r\n        msg = '请求格式不正确'\r\n        break\r\n      case 500:\r\n        msg = '服务器端出错'\r\n        break\r\n    }\r\n    message.error(msg);\r\n    return Promise.reject(error);\r\n  },\r\n);\r\n\r\n// 统一发起请求的函数\r\nexport function request<T>(options: AxiosRequestConfig) {\r\n  return axios.request<T>(options);\r\n}\r\n","C:\\Users\\limingyi\\Desktop\\vue3\\react_todo\\src\\api\\index.ts",[],"C:\\Users\\limingyi\\Desktop\\vue3\\react_todo\\src\\store\\module\\settings.ts",[],"C:\\Users\\limingyi\\Desktop\\vue3\\react_todo\\src\\views\\menu\\menu2\\menu2_2\\index.tsx",["167","168"],"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport { Table, Tag, Space } from 'antd';\r\n\r\nfunction Menu2_2() {\r\n    const { Column, ColumnGroup } = Table;\r\n\r\n    const data = [\r\n        {\r\n            key: '1',\r\n            firstName: 'John',\r\n            lastName: 'Brown',\r\n            age: 32,\r\n            address: 'New York No. 1 Lake Park',\r\n            tags: ['nice', 'd